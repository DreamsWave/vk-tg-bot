{
	"root": "packages/telegram-api",
	"sourceRoot": "packages/telegram-api/src",
	"projectType": "library",
	"targets": {
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["packages/telegram-api/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/packages/telegram-api"],
			"options": {
				"jestConfig": "packages/telegram-api/jest.config.js",
				"passWithNoTests": true
			}
		}
	},
	"tags": []
}
